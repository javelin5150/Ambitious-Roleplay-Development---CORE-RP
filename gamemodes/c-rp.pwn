	    {
	        return SendErrorMessage(playerid, "There is no fuel in this pump.");
		}
		else
		{
	        Players[playerid][pRefuel] = id;
            Timers[playerid][tRefuel] = SetTimerEx("RefuelCycle", 1000, true, "iii", playerid, vehicleid, id);

