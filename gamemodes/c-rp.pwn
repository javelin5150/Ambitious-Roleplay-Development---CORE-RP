						{
						    Houses[Players[playerid][pPickID]][hLocked] = 0;
						    SaveHouse(Players[playerid][pPickID]);

						    StopPicking(playerid);
						    SendInfoMessage(playerid, "You have picked open the lock. %i", Players[playerid][pPickID]);
