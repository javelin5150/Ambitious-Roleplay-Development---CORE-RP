forward DecreasePower(playerid);
public DecreasePower(playerid)
{
    new
		Float:value = GetPlayerProgressBarValue(playerid, Players[playerid][pBars][0]);

	switch (Players[playerid][pWorkout])
	{
	    case WORKOUT_DUMBELLS:
	    {
