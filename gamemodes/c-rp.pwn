OpenGate(playerid, gate)
{
    if (IsValidGateID(gate))
	{
 	    GateHandler(gate);

	    if (Gates[gate][gLinkID] > 0) GateHandler(GetGateID(Gates[gate][gLinkID]));

		if (Gates[gate][gOpened])
		{
		    SendInfoMessage(playerid, "You have opened the gate.");
		}
