	} else if (type == 1) {
		package = PACKAGE_TYPE_PISTOLS;
	} else if (type == 2) {
		package = PACKAGE_TYPE_SHOTGUNS;
	} else if (type == 3) {
		package = PACKAGE_TYPE_SMGS;
	} else if (type == 4) {
		package = PACKAGE_TYPE_RIFLES;
