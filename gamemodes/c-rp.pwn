					}
	            }
	        }
	        case CHOOSE_TYPE_TRUNK:
	        {
	            new
					vehicle = GetVehicleID(GetClosestVehicle(playerid));

	            Players[playerid][pChoose] = CHOOSE_TYPE_NONE;

	            if (IsValidVehicleID(vehicle))
	            {
