	return 0.0;
}

Float:GetVehicleMPH(vehicleid)
{
	new
	    Float:fX,
	    Float:fY,
	    Float:fZ;

	if (GetVehicleVelocity(vehicleid, fX, fY, fZ))
	{
		return floatsqroot( fX*fX + fY*fY + fZ*fZ ) * 180 / 1.609344; // Mph
	}
	return 0.0;
}


