			}
			else
			{
		        format(gExecute, sizeof(gExecute), "SELECT vID FROM rp_vehicles WHERE vPlate = '%s'", mysql_escaped(value));
		        mysql_tquery(gConnection, gExecute, "OnPlayerCheckPlate", "is", playerid, value);
