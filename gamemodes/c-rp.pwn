				format(gExecute, sizeof(gExecute), "SELECT Warrants FROM rp_accounts WHERE Username = '%s'", gTargetName[playerid]);
			    mysql_tquery(gConnection, gExecute, "OnPlayerGetWarrants", "i", playerid);
