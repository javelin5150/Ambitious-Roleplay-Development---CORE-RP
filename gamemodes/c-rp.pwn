      	Float:z,
       	Float:angle
	;

	GetPlayerPos(playerid, x, y, z);
 	GetPlayerFacingAngle(playerid, angle);

	Players[playerid][pAddVehicle] = SpawnVehicle(modelid, x, y, z, angle, color1, color2, -1, 0);

