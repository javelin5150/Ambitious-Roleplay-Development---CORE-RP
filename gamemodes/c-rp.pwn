GetClosestVehicle(playerid)
{
	new
		Float:x,
		Float:y,
		Float:z;

	for (new i = 1; i < MAX_VEHICLES; i ++)
	{
	    if (!IsValidVehicle(i) || !IsVehicleStreamedIn(i, playerid))
		{
			continue;
	    }
	    else
	    {
			GetVehiclePos(i, x, y, z);

			if (IsPlayerInRangeOfPoint(playerid, 3.7, x, y, z))
			{
				return i;
			}
		}
	}
	return INVALID_VEHICLE_ID;
}

