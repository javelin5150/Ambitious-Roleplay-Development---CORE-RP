							    {
			    	                case 0: SetCheckpoint(playerid, CHECKPOINT_JOB, 2790.8264, -2410.4639, 13.6329, 3.0);
			    	                case 1: SetCheckpoint(playerid, CHECKPOINT_JOB, 2790.8264, -2410.4639, 13.6329, 3.0);
								}

