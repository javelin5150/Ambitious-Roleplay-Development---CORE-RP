	}
	else
	{
		new
			owner,
			ownername[MAX_PLAYER_NAME],
			value,
			forsale,
			Float:x,
			Float:y,
			Float:z,
			string[512];

		new adr[32];

		owner = cache_get_field_content_int(0, "hOwner");
		value = cache_get_field_content_int(0, "hValue");
		forsale = cache_get_field_content_int(0, "hForSale");
