{
    if (GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_CARRY)
	{
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
		RemovePlayerAttachedObject(playerid, ATTACHED_SLOT_MISC);
