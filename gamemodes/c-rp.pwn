Dialog:ManageWarrants(playerid, response, listitem, inputtext[])
{
	if (!IsPlayerInFaction(playerid, FACTION_LEO))
	{
		return 0;
	}
	if (response)
	{
		switch (listitem)
		{
			case 0:
