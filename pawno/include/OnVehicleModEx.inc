stock GetComponentName(componentid, name[], len = sizeof(name))
{
	if (MIN_COMPONENT_ID <= componentid <= MAX_COMPONENT_ID)
	{
		componentid -= MIN_COMPONENT_ID;
	
		return strunpack(name, s_ComponentNames[componentid], len);
	}
	
	name[0] = '\0';
	
	return 0;
}

stock IsPlayerInModShop(playerid)
{
	if (IsPlayerConnected(playerid))
	{
		return s_bModShopStatus{playerid};
	}

	return 0;
}

IsValidComponentForVehicle(vehicleid, componentid)
{
	new modelid = GetVehicleModel(vehicleid);

	// Check for nitrous, wheels, and hydraulics.
	if ((1008 <= componentid <= 1010) || (componentid == 1025) || (1073 <= componentid <= 1087) || (1096 <= componentid <= 1098))
	{
		switch (modelid)
		{
			case 581, 523, 462, 521, 463, 522, 461, 448, 468, 586, 509, 481, 510, 472, 473, 493, 595, 484, 430, 453, 452, 446, 454, 590, 569, 537, 538, 570, 449:
			{
			    return false;
			}
			default:
			{
				return true;
			}
		}
	}
	else
	{
		modelid -= 400;
	
	    for (new i = 0; i < sizeof(s_ValidComponentArray[]); i ++)
	    {
	        if (s_ValidComponentArray[modelid][i] == componentid)
	        {
	            return true;
	        }
		}
	}
	return false;
}

GetVehicleComponentPrice(componentid)
{
	for (new i = 0; i < sizeof(s_ComponentPriceArray); i ++)
	{
	    if (s_ComponentPriceArray[i][0] == componentid)
	    {
	        return s_ComponentPriceArray[i][1];
		}
	}
	
	return 0;
}

public OnPlayerConnect(playerid)
{
	s_bModShopStatus{playerid} = false;
	
	#if defined VM_OnPlayerConnect
		return VM_OnPlayerConnect(playerid);
	#else
		return 1;
	#endif
}

public OnEnterExitModShop(playerid, enterexit, interiorid)
{
	s_bModShopStatus{playerid} = enterexit;
	
	#if defined VM_OnEnterExitModShop
	    return VM_OnEnterExitModShop(playerid, enterexit, interiorid);
	#else
	    return 1;
	#endif
}

public OnVehicleMod(playerid, vehicleid, componentid)
{
	if (!IsValidComponentForVehicle(vehicleid, componentid) || !s_bModShopStatus{playerid} || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
	{
	    CallLocalFunction("OnVehicleModEx", "iiiii", playerid, vehicleid, componentid, GetVehicleComponentPrice(componentid), true);
	    return 0;
	}

	CallLocalFunction("OnVehicleModEx", "iiiii", playerid, vehicleid, componentid, GetVehicleComponentPrice(componentid), false);

	#if defined VM_OnVehicleMod
	    return VM_OnVehicleMod(playerid, vehicleid, componentid);
	#else
	    return 1;
	#endif
}

